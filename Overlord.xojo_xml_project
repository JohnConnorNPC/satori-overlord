  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function HandleURL(request As WebRequest, response As WebResponse) As Boolean</SourceLine>
   <SourceLine>// Log request details</SourceLine>
   <SourceLine>System.DebugLog("Path: " + request.Path)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle check-in requests</SourceLine>
   <SourceLine>If request.Path.BeginsWith("checkin/") Then</SourceLine>
   <SourceLine>// Extract the hostname from the request path</SourceLine>
   <SourceLine>Var hostname As String = request.Path.NthField("/", 2).Lowercase</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Store the IP address associated with the hostname</SourceLine>
   <SourceLine>ipaddresses.Value(hostname) = request.RemoteAddress</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Add check-in details to the database</SourceLine>
   <SourceLine>AddCheckin(hostname, request.Body, request.RemoteAddress)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Check for any pending commands for this hostname</SourceLine>
   <SourceLine>Var commandInfo As Dictionary = CheckForPendingCommands(hostname)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Prepare response with a command if available</SourceLine>
   <SourceLine>Dim out As String </SourceLine>
   <SourceLine>If commandInfo &lt;&gt; Nil Then</SourceLine>
   <SourceLine>Select Case commandInfo.Value("commandtype")</SourceLine>
   <SourceLine>Case "command"</SourceLine>
   <SourceLine>out = "command:" + commandInfo.Value("guid") + ":" + commandInfo.Value("command")</SourceLine>
   <SourceLine>Case "py"</SourceLine>
   <SourceLine>out = "py:" + commandInfo.Value("guid") + ":" + commandInfo.Value("command")</SourceLine>
   <SourceLine>Case "sh"</SourceLine>
   <SourceLine>out = "sh:" + commandInfo.Value("guid") + ":" + commandInfo.Value("command")</SourceLine>
   <SourceLine>End Select</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>System.DebugLog(out)</SourceLine>
   <SourceLine>response.Write(out)</SourceLine>
   <SourceLine>response.Status = 200</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Return True</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle callback requests</SourceLine>
   <SourceLine>ElseIf request.Path.BeginsWith("callback/") Then</SourceLine>
   <SourceLine>// Extract the GUID from the request path</SourceLine>
   <SourceLine>Var guid As String = request.Path.NthField("/", 2)</SourceLine>
   <SourceLine>Print(request.Body)</SourceLine>
   <SourceLine>// Update command execution result in the database</SourceLine>
   <SourceLine>UpdateCommandExecutionResult(guid, request.Body)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>response.Status = 200</SourceLine>
   <SourceLine>Return True</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Serve .sh or .py files</SourceLine>
   <SourceLine>ElseIf request.Path.BeginsWith("files/") And (request.Path.EndsWith(".sh") Or request.Path.EndsWith(".py")) Then</SourceLine>
   <SourceLine>Var filename As String = request.Path.NthField("/", 2)</SourceLine>
   <SourceLine>Dim f As New FolderItem(filename)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If f.Exists And f.IsReadable Then</SourceLine>
   <SourceLine>// Open the requested file and read its contents</SourceLine>
   <SourceLine>Dim t As TextInputStream = TextInputStream.Open(f)</SourceLine>
   <SourceLine>Dim s As String = t.ReadAll</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Replace placeholders in the script with actual values</SourceLine>
   <SourceLine>s = s.ReplaceAll("##username##", KeyStore.GetValue("username", "username"))</SourceLine>
   <SourceLine>s = s.ReplaceAll("##satori_path##", KeyStore.GetValue("satori_path", "satori_path"))</SourceLine>
   <SourceLine>s = s.ReplaceAll("##server_url##", KeyStore.GetValue("server_url", "server_url"))</SourceLine>
   <SourceLine>s = s.ReplaceAll("##vault_pw##", KeyStore.GetValue("vault_pw", "vault_pw"))</SourceLine>
   <SourceLine>s = s.ReplaceAll("##client_trigger##", KeyStore.GetValue("client_trigger", "30"))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>response.Write(s)</SourceLine>
   <SourceLine>response.Status = 200</SourceLine>
   <SourceLine>Return True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// If no valid path is recognized, return False</SourceLine>
   <SourceLine>Return False</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
