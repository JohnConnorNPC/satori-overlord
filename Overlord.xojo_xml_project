 <Method>
  <ItemName>AddCheckin</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>2065629183</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub AddCheckin(hostname as String, metrics as String, ip_address as string)</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>hostname = hostname.Lowercase</SourceLine>
   <SourceLine>// SQL query to insert check-in data if the hostname doesn't already exist</SourceLine>
   <SourceLine>//Dim sql As String = "INSERT INTO checkins (hostname, checkin_time, ip_address) SELECT ?, ?, ? WHERE NOT EXISTS (SELECT 1 FROM checkins WHERE hostname = ?);"</SourceLine>
   <SourceLine>Dim sql As String = "INSERT INTO checkins (hostname, checkin_time, ip_address) VALUES (?, ?, ?) ON CONFLICT(hostname) DO UPDATE SET checkin_time = excluded.checkin_time, ip_address = excluded.ip_address;"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Execute the SQL statement with the relevant parameters</SourceLine>
   <SourceLine>db.ExecuteSQL(sql, hostname, DateTime.Now.SecondsFrom1970, ip_address)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Get or initialize the current metrics for the hostname</SourceLine>
   <SourceLine>Dim current_metrics As New Dictionary</SourceLine>
   <SourceLine>If host_metrics.HasKey(hostname) Then</SourceLine>
   <SourceLine>current_metrics = host_metrics.Value(hostname)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Parse the JSON metrics and update the current metrics dictionary</SourceLine>
   <SourceLine>Dim j As New JSONItem(metrics)</SourceLine>
   <SourceLine>For i As Integer = 0 To j.Count - 1</SourceLine>
   <SourceLine>current_metrics.Value(j.KeyAt(i)) = j.Value(j.KeyAt(i))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Debugging: print values that contain "satori" for further inspection</SourceLine>
   <SourceLine>If j.KeyAt(i).Contains("satori") Then </SourceLine>
   <SourceLine>Print j.KeyAt(i) + ":" + j.Value(j.KeyAt(i))</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Store the updated metrics back into the host_metrics dictionary</SourceLine>
   <SourceLine>host_metrics.Value(hostname) = current_metrics</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Catch error As DatabaseException</SourceLine>
   <SourceLine>// Log any database errors during the check-in process</SourceLine>
   <SourceLine>System.DebugLog("Error adding check-in: " + error.Message)</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
