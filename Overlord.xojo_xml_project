   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub InitializeDatabase()</SourceLine>
   <SourceLine>// Define the database file path</SourceLine>
   <SourceLine>Var dbFile As New FolderItem("manager_database.sqlite")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//If dbFile.Exists=False Then db.CreateDatabase</SourceLine>
   <SourceLine>db = new SQLiteDatabase</SourceLine>
   <SourceLine>db.DatabaseFile = dbFile</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If Not dbFile.Exists Then</SourceLine>
   <SourceLine>If Not db.CreateDatabaseFile Then</SourceLine>
   <SourceLine>MessageBox("Error: Could not create the database file. " + db.ErrorMessage)</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Try to connect to the database</SourceLine>
   <SourceLine>If Not db.Connect Then</SourceLine>
   <SourceLine>MessageBox("Error: Could not connect to the database. " + db.ErrorMessage)</SourceLine>
   <SourceLine>Return</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Begin a transaction</SourceLine>
   <SourceLine>db.SQLExecute("BEGIN TRANSACTION")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Create the necessary tables if they don't exist</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>// Example: Create a table to store check-ins</SourceLine>
   <SourceLine>db.SQLExecute("CREATE TABLE IF NOT EXISTS checkins (id INTEGER PRIMARY KEY, hostname TEXT UNIQUE, checkin_time double, ip_address TEXT )")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Example: Create a table to store command executions</SourceLine>
   <SourceLine>db.SQLExecute("CREATE TABLE IF NOT EXISTS command_executions (id INTEGER PRIMARY KEY, guid TEXT, hostname TEXT, commandtype TEXT, command TEXT, execution_time double, result TEXT, status TEXT DEFAULT 'pending')")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Commit the transaction</SourceLine>
   <SourceLine>db.SQLExecute("COMMIT")</SourceLine>
   <SourceLine>Catch error As DatabaseException</SourceLine>
   <SourceLine>// Rollback the transaction in case of an error</SourceLine>
   <SourceLine>db.SQLExecute("ROLLBACK")</SourceLine>
   <SourceLine>MessageBox("Error: " + error.Message)</SourceLine>
   <SourceLine>End Try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
