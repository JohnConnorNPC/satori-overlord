   <SourceLine>Sub UpdateHostnameListBox(listbox As WebListBox)</SourceLine>
   <SourceLine>Try</SourceLine>
   <SourceLine>// Clear the current contents of the ListBox</SourceLine>
   <SourceLine>//listbox.RemoveAllRows()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Query to select distinct hostnames and their last check-in time</SourceLine>
   <SourceLine>Var sql As String = "SELECT hostname, MAX(checkin_time) AS last_checkin FROM checkins GROUP BY hostname ORDER BY last_checkin ASC"</SourceLine>
   <SourceLine>Var rs As RowSet = db.SelectSQL(sql)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim hosts As New Dictionary</SourceLine>
   <SourceLine>Dim latehosts As New Dictionary</SourceLine>
   <SourceLine>While rs &lt;&gt; Nil And Not rs.AfterLastRow</SourceLine>
   <SourceLine>//listbox.AddRow(rs.Column("hostname").StringValue)</SourceLine>
   <SourceLine>hosts.Value(rs.Column("hostname").StringValue)=datetime(new datetime(rs.Column("last_checkin").DoubleValue)).SQLDateTime</SourceLine>
   <SourceLine>Dim d As Double = rs.Column("last_checkin").DoubleValue</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim late_minion As Integer = KeyStore.GetValue("client_trigger","60").Val * 4</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If d &lt; DateTime.Now.SecondsFrom1970 - late_minion Then latehosts.Value(rs.Column("hostname").StringValue)=rs.Column("hostname").StringValue</SourceLine>
   <SourceLine>'System.DebugLog(Format(d,"################"))</SourceLine>
   <SourceLine>'System.DebugLog(Format(DateTime.now.SecondsFrom1970,"################"))</SourceLine>
