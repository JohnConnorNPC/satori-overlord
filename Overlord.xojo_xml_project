   <SourceLine>result.Value("command") = rs.Column("command").StringValue</SourceLine>
   <SourceLine>result.Value("commandtype") = rs.Column("commandtype").StringValue</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Update the command status to in-progress</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If result.Value("command") = "update_minion.sh" Or result.Value("command") = "restart_minion.sh"Then</SourceLine>
   <SourceLine>UpdateCommandStatusToInProgress(result.Value("guid"),"completed")</SourceLine>
