    <SourceLine>Sub ContextualMenuSelected(hitItem As WebMenuItem)</SourceLine>
    <SourceLine>' Handle menu item actions based on user selection in the context menu</SourceLine>
    <SourceLine>If Me.SelectedRowIndex = -1 Then Return</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Perform an action based on the selected menu item</SourceLine>
    <SourceLine>Select Case hitItem.Text</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Restart the Satori service</SourceLine>
    <SourceLine>Case "Restart Satori"</SourceLine>
    <SourceLine>Dim command As String = "systemctl restart satori.service"</SourceLine>
    <SourceLine>RunCommandOnAllSelected(ListBox1, command)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Open the wallet interface for the selected IP address</SourceLine>
    <SourceLine>Case "Open Wallet"</SourceLine>
    <SourceLine>If ipaddresses.haskey(Me.CellTextAt(Me.SelectedRowIndex, 0)) Then</SourceLine>
    <SourceLine>Dim ipaddress As String = ipaddresses.Value(Me.CellTextAt(Me.SelectedRowIndex, 0))</SourceLine>
    <SourceLine>GoToURL("http://" + ipaddress + ":24601/wallet/main", True)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Open the vault interface for the selected IP address</SourceLine>
    <SourceLine>Case "Open Vault"</SourceLine>
    <SourceLine>If ipaddresses.haskey(Me.CellTextAt(Me.SelectedRowIndex, 0)) Then</SourceLine>
    <SourceLine>Dim ipaddress As String = ipaddresses.Value(Me.CellTextAt(Me.SelectedRowIndex, 0))</SourceLine>
    <SourceLine>GoToURL("http://" + ipaddress + ":24601/vault", True)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Refresh the list of hostnames</SourceLine>
    <SourceLine>Case "Refresh"</SourceLine>
    <SourceLine>UpdateHostnameListBox(Me)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Reboot the system</SourceLine>
    <SourceLine>Case "Reboot"</SourceLine>
    <SourceLine>WebDialog_Reboot1.Show</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Shutdown the system</SourceLine>
    <SourceLine>Case "Shutdown"</SourceLine>
    <SourceLine>WebDialog_Shutdown1.Show</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Delete selected entries from the database and refresh the list</SourceLine>
    <SourceLine>Case "Delete"</SourceLine>
    <SourceLine>' Select all from the database, remove the selected entries, clear the list box, and refresh.</SourceLine>
    <SourceLine>' Implementation needed here.</SourceLine>
    <SourceLine>Case "View"</SourceLine>
    <SourceLine>Dim m As New Container_Minion</SourceLine>
    <SourceLine>m.ShowPopover(Label1)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Select</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
