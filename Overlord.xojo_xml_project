   <SourceLine>If mymetrics.HasKey("satori_installed") Then</SourceLine>
   <SourceLine>Dim satori_installed As String = mymetrics.Value("satori_installed")</SourceLine>
   <SourceLine>If listbox.CellTextAt(i,2) &lt;&gt; satori_installed Then listbox.CellTextAt(i,2) = satori_installed</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
