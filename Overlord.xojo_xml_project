   <SourceLine>If hosts.HasKey(s) Then</SourceLine>
   <SourceLine>Dim lbt As String</SourceLine>
   <SourceLine>Dim hn As String = hosts.Value(s).StringValue</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If s=listbox.CellTextAt(i,0).StringValue Then</SourceLine>
   <SourceLine>If listbox.CellTextAt(i,1) IsA WebListBoxCellRenderer Then</SourceLine>
   <SourceLine>Dim w As WebListBoxStyleRenderer = listbox.CellTextAt(i,1)</SourceLine>
   <SourceLine>lbt = w.Value</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>lbt = listbox.CellTextAt(i,1)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If hn &lt;&gt; lbt Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>listbox.CellTextAt(i,1) =  hosts.Value(s).StringValue</SourceLine>
   <SourceLine>If latehosts.HasKey(s) Then</SourceLine>
   <SourceLine>Dim w As New WebStyle</SourceLine>
   <SourceLine>w.ForegroundColor=Color.red</SourceLine>
   <SourceLine>listbox.CellTextAt(i,1) =New WebListBoxStyleRenderer(w,hosts.Value(s).StringValue)</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>Dim w As New WebStyle</SourceLine>
   <SourceLine>listbox.CellTextAt(i,1) =New WebListBoxStyleRenderer(w,hosts.Value(s).StringValue)</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>&#32;</SourceLine>
   <SourceLine>If host_metrics.HasKey(s) Then</SourceLine>
   <SourceLine></SourceLine>
