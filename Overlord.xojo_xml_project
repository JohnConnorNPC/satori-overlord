  <HookInstance>
   <ItemName>ContextualMenuSelected</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>977848319</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub ContextualMenuSelected(hitItem As WebMenuItem)</SourceLine>
    <SourceLine>' Check if no row is selected, return if true</SourceLine>
    <SourceLine>If Me.SelectedRowIndex = -1 Then Return</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim s As String = Me.CellTextAt(Me.SelectedRowIndex, 0)</SourceLine>
    <SourceLine>s = s.Trim</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Handle different actions based on the selected item's text</SourceLine>
    <SourceLine>Select Case hitItem.Text</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Case "Run"</SourceLine>
    <SourceLine>' Ensure a row is selected in ListBox1</SourceLine>
    <SourceLine>If listbox1.SelectedRowIndex = -1 Then Return</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Get the text from the selected row and trim whitespace</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If s.IsEmpty Then Return</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Run the command based on the file extension</SourceLine>
    <SourceLine>If s.EndsWith(".sh") Then</SourceLine>
    <SourceLine>RunCommandOnAllSelected(ListBox1, s, "sh")</SourceLine>
    <SourceLine>ElseIf s.EndsWith(".py") Then</SourceLine>
    <SourceLine>RunCommandOnAllSelected(ListBox1, s, "py")</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Case "Edit"</SourceLine>
    <SourceLine>' Trim the selected text and open it in the file editor if it exists</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If s.EndsWith(".sh") Or s.EndsWith(".py") Or s.EndsWith(".txt")  Then</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim w As New Container_FileEditor</SourceLine>
    <SourceLine>Dim f As New FolderItem(s)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If f.Exists Then</SourceLine>
    <SourceLine>w.file = f</SourceLine>
    <SourceLine>w.ShowPopover(Me)</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Case "Archive"</SourceLine>
    <SourceLine>' Archive the file if it exists, then update the plugins list</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim f As New FolderItem(s)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If f.Exists Then ArchiveFile(f)</SourceLine>
    <SourceLine>UpdatePluginsListbox(listbox2)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Case "Hide/Unhide"</SourceLine>
    <SourceLine>' Toggle the hidden state of the selected item</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If KeyStore.GetValue(s) = "Hidden" Then</SourceLine>
    <SourceLine>KeyStore.RemoveKey(s)</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>KeyStore.SetValue(s, "Hidden")</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>UpdatePluginsListbox(listbox2)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Select</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
