 <Method>
  <ItemName>UpdatePluginsListbox</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1423806463</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub UpdatePluginsListbox(WebListBox as weblistbox)</SourceLine>
   <SourceLine>Var currentFolder As FolderItem = New FolderItem</SourceLine>
   <SourceLine>weblistbox.RemoveAllRows()</SourceLine>
   <SourceLine>// Loop through all files in the current folder</SourceLine>
   <SourceLine>For Each file As folderitem In currentFolder.Children</SourceLine>
   <SourceLine>If file.IsFolder=False Then</SourceLine>
   <SourceLine>// Check if the file is a .py file</SourceLine>
   <SourceLine>If (file.Name.Right(3) = ".py" Or file.Name.Right(3) = ".sh" Or file.Name.Right(3) = ".zip" Or file.Name.Right(3) = ".7z") Then</SourceLine>
   <SourceLine>// Add the file name to the WebListBox</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If KeyStore.GetValue(file.name)&lt;&gt;"Hidden" or Checkbox_ShowHidden.Value Then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>WebListBox.AddRow(file.Name)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
